pipeline {
   agent any

 

   stages {
        
      stage('Validation') {
         steps {
            sh 'rm -r techbleat-website || true' 

         }
      }

      stage('copy') {
         steps {

            sh 'sudo cp devops.jpg /var/www/html'   
         }
      }

       stage('copy html') {
         steps {
            sh 'sudo cp index.html  /var/www/html'
         }
      }

   }

}